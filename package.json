{
  "name": "wr-float-denomination-tracker",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "prestart": "npm run build",
    "start": "cd dist/js && node index.js",
    "test": "npm run build && mocha dist/js/test/integration/routes.auth.test.js --exit",
    "test:forceunlock": "npm run build && npm run migrate:unlock && mocha dist/js/test/integration/routes.auth.test.js --exit",
    "test:watch": "npm run build && mocha -w dist/js/test/integration/routes.auth.test.js",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "ts_watch": "tsc -w",
    "client_dev": "cd client && npm run start",
    "server_auto_restart": "nodemon dist/js/index.js",
    "client_build": "cd client && npm run build && cp -r build ../dist/build",
    "server_dev": "concurrently \"npm run ts_watch\" \"npm run server_auto_restart\"",
    "development": "concurrently \"npm run ts_watch\" \"npm run client_dev\"",
    "dev": "concurrently \"npm run ts_watch\" \"npm run server_auto_restart\" \"npm run client_dev\"",
    "migrate:latest": "cd dist/js && knex migrate:latest",
    "migrate:rollback": "cd dist/js && knex migrate:rollback",
    "migrate:unlock": "cd dist/js && knex migrate:unlock",
    "seed": "cd dist/js && knex seed:run",
    "build_db": "touch dist/js/db/wr-float-denomination-tracker.db3 && npm run migrate:latest && npm run seed"
  },
  "author": "Robyn Felicity Heather Veitch <robynfhveitch@gmail.com> (https://robynveitch.com/)",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-session": "^1.17.2",
    "knex": "^0.95.11",
    "mocha": "^8.4.0",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "objection": "^2.2.17",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/carbon-components-react": "^7.44.1",
    "@types/chai": "^4.2.22",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/express-flash": "0.0.2",
    "@types/express-session": "^1.17.4",
    "@types/knex": "^0.16.1",
    "@types/mocha": "^8.2.3",
    "@types/morgan": "^1.9.3",
    "@types/passport": "^1.0.7",
    "@types/passport-local": "^1.0.34",
    "concurrently": "^6.3.0",
    "cors": "^2.8.5",
    "passport-stub": "^1.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.4.4"
  }
}
